    event open (string a1 , int a2 , unsigned int a3);

    int b,c;
   
    bool d=false;
  
    unsigned int y; 
    string s2;
    
    class CString;
    CString cs;
    
    string file ;
    int flags;
    unsigned int mode;

    int g(int a, int b, double x);

    double f(string s1, CString s2);

    open(file, flags, mode) -> {
           g(b,c,y); 
           f(s2, cs);		         
 		
	   g(0.5, y, d);
	   f(cs, s2); 
    };
   

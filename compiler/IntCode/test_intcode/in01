    event a(int a);
int fib(int n)
{
	int c;
	if(n==1)
		return 0;
	if(n==2)
		return 1;
	c=fib(n-2)+fib(n-1);
	return c;
};

    int n = 5 ;
	int k;
a(k) -> {
fib(n);};

